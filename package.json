  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/atom/electron-quick-start/issues"
  },
  "homepage": "https://github.com/atom/electron-quick-start#readme",
  "devDependencies": {
    "electron-prebuilt": "^0.35.4"
  },
  "dependencies": {
    "bluebird": "^3.0.6",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1",
    "openpgp": "^1.3.0"
  }
}
