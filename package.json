{
  "name": "modmail-app",
  "version": "1.0.0",
  "description": "Extremely moddable Gmail",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "build-mac": "electron-packager . Modmail --platform=darwin --arch=x64 --version=0.35.4 --icon images/logo.icns",
    "build-windows": "electron-packager . Modmail --platform=win32 --arch=x64 --version=0.35.4 --icon images/logo.ico",
    "build-linux": "electron-packager . Modmail --platform=linux --arch=x64 --version=0.35.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atom/electron-quick-start.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial"
  ],
